<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://gwtproject.org/doctype/2.7.0/gwt-module.dtd">
<module>
	<inherits name='com.google.gwt.user.User' />
	<inherits name='fr.lteconsulting.hexa.HexaUtils' />

	<super-source path="emul/classinfo" />
	
	<source path="classinfo" />

	<generate-with class="fr.lteconsulting.hexa.rebind.classinfo.ClazzGenerator">
		<when-type-assignable class="fr.lteconsulting.hexa.classinfo.Clazz" />
	</generate-with>

	<generate-with class="fr.lteconsulting.hexa.rebind.classinfo.ClazzBundleGenerator">
		<when-type-assignable class="fr.lteconsulting.hexa.classinfo.gwt.ClazzBundle" />
	</generate-with>
</module>