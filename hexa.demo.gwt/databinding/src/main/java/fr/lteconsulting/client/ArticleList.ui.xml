<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:hexa="urn:import:fr.lteconsulting.hexa.client.ui.widget">
    <ui:with type="fr.lteconsulting.hexa.client.css.bindings.BootstrapHexaCss"
             field="bs"></ui:with>
    <g:HTMLPanel stylePrimaryName="{bs.well}">
        <div class="{bs.panel} {bs.panelDefault}">
            <div class="{bs.panelHeading}">
                <h3 class="{bs.panelTitle}">Article list</h3>
            </div>
            <div class="{bs.panelBody}">
                <hexa:ListBox ui:field="listBox" stylePrimaryName="{bs.formControl}"></hexa:ListBox>
                <g:Button ui:field="add" stylePrimaryName="{bs.btn} {bs.btnSuccess}">Add new</g:Button>
                <g:Button ui:field="delete" stylePrimaryName="{bs.btn} {bs.btnWarning}">Delete</g:Button>
                <div ui:field="listDiv" class="{bs.listGroup}"></div>
            </div>
        </div>
    </g:HTMLPanel>
</ui:UiBinder> 